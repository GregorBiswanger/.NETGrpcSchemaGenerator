{
  "executable": "DotNetGrpcSchemaGenerator",
  "splashscreen": {
    "imageFile": "/wwwroot/img/splashscreen.jpg"
  },
  "name": "DotNetGrpcSchemaGenerator",
  "author": "Gregor Biswanger",
  "singleInstance": false,
  "environment": "Production",
  "build": {
    "appId": "com.DotNetGrpcSchemaGenerator.app",
    "productName": "DotNetGrpcSchemaGenerator",
    "copyright": "Copyright Â© 2020",
    "buildVersion": "1.0.0",
    "compression": "maximum",
    "win": {
      "icon": "bin/Assets/DotNetGrpcSchemaGenerator.ico"
    },
    "directories": {
      "output": "../../../bin/Desktop"
    },
    "extraResources": [
      {
        "from": "./bin",
        "to": "bin",
        "filter": [ "**/*" ]
      }
    ],
    "files": [
      {
        "from": "./ElectronHostHook/node_modules",
        "to": "ElectronHostHook/node_modules",
        "filter": [ "**/*" ]
      },
      "**/*"
    ]
  }
}